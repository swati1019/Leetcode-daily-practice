#include <iostream>
#include <map>
using namespace std;
int main(){
    int n;
    cin>>n;
    vector<int> v,nv;
    for(int i=0; i<n; i++){
        int m;
        cin>>m;
        v.push_back(m);
    }
    int c=0;
    for(int i=0; i<n; i++){
        if(v[i]==0) c++;
        else nv.push_back(v[i]);
    }
    while(c--){
        nv.push_back(0);
    }
    for(int i=0;i<n; i++){
        cout<<nv[i];
    }

 
}
