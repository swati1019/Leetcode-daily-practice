#include <iostream>
#include <map>
using namespace std;
int main(){
    int n,n2;
    cin>>n;
    vector<int> v,v2,uv;
    for(int i=0; i<n; i++){
        int m;
        cin>>m;
        v.push_back(m);
    }
    cin>>n2;
    for(int i=0; i<n2; i++){
        int m; 
        cin>>m; 
        v2.push_back(m);
    }
    for(int i=0; i<n; i++){
        if(v[i]!=v[i+1]) uv.push_back(v[i]);
        else continue;
    }
    for(int i=0; i<n2; i++){
        auto it = find(uv.begin() , uv.end(), v2[i]);
        if( it == uv.end()) uv.push_back(v2[i]);
        else continue;
    }
    sort(uv.begin(), uv.end());
    for(int i=0 ; i<uv.size() ; i++){
        cout<<uv[i];
    }

 
}
